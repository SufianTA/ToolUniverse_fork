        ],
        "tool_call": {
          "name": "OpenTargets_get_drug_mechanisms_of_action_by_chemblId",
          "arguments": {
            "chemblId": "CHEMBL941"
          }
        },
        "validations": [
          {
            "type": "json_path_equals",
            "path": "data.drug.name",
            "value": "IMATINIB"
          },
          {
            "type": "json_path_non_empty",
            "path": "data.drug.mechanismsOfAction.rows"
          }
        ]
      },
      {
        "query": "Diseases associated with imatinib",
        "expected_tools": [
          "OpenTargets_get_associated_diseases_by_drug_chemblId"
        ],
        "tool_call": {
          "name": "OpenTargets_get_associated_diseases_by_drug_chemblId",
          "arguments": {
            "chemblId": "CHEMBL941"
          }
        },
        "validations": [
          {
            "type": "json_path_non_empty",
            "path": "data.drug.linkedDiseases.rows"
          }
        ]
      },
      {
        "query": "Find molecules similar to imatinib in ChEMBL",
        "expected_tools": [
          "ChEMBL_search_similar_molecules"
        ],
        "tool_call": {
          "name": "ChEMBL_search_similar_molecules",
          "arguments": {
            "query": "imatinib",
            "similarity_threshold": 80,
            "max_results": 5
          }
        },
        "validations": [
          {
            "type": "json_path_equals",
            "path": "[0].chembl_id",
            "value": "CHEMBL941"
          },
          {
            "type": "json_path_non_empty",
            "path": "[0].similar_molecules"
          }
        ]
      }
    ]
