[
  {
    "id": "app_uniprot_literature",
    "title": "APP protein function and literature",
    "context": "Validate APP protein annotations and link to literature evidence.",
    "seed_tool": "UniProt_get_entry_by_accession",
    "chain_expected": [
      "UniProt_get_function_by_accession",
      "EuropePMC_search_articles"
    ],
    "steps": [
      {
        "query": "Swiss-Prot entry for amyloid beta precursor protein (APP) human accession",
        "expected_tools": [
          "UniProt_get_entry_by_accession"
        ],
        "tool_call": {
          "name": "UniProt_get_entry_by_accession",
          "arguments": {
            "accession": "P05067"
          }
        },
        "validations": [
          {
            "type": "json_path_equals",
            "path": "primaryAccession",
            "value": "P05067"
          },
          {
            "type": "json_path_contains",
            "path": "entryType",
            "value": "Swiss-Prot"
          }
        ]
      },
      {
        "query": "Functional annotation of APP protein from UniProt (neuronal/synaptogenesis)",
        "expected_tools": [
          "UniProt_get_function_by_accession"
        ],
        "tool_call": {
          "name": "UniProt_get_function_by_accession",
          "arguments": {
            "accession": "P05067"
          }
        },
        "validations": [
          {
            "type": "json_path_contains",
            "path": "[0]",
            "value": "synaptogenesis"
          }
        ]
      },
      {
        "query": "Literature on amyloid beta precursor protein and Alzheimer disease",
        "expected_tools": [
          "EuropePMC_search_articles"
        ],
        "tool_call": {
          "name": "EuropePMC_search_articles",
          "arguments": {
            "query": "amyloid beta precursor protein Alzheimer disease",
            "limit": 5
          }
        },
        "validations": [
          {
            "type": "json_path_non_empty",
            "path": "[0].title"
          },
          {
            "type": "json_path_contains",
            "path": "[0].title",
            "value": "amyloid"
          }
        ]
      }
    ]
  },
  {
    "id": "egfr_cellular_context",
    "title": "EGFR cellular context",
    "context": "Combine HPA and Open Targets to validate EGFR localization, ontology, and interactions.",
    "seed_tool": "HPA_get_gene_basic_info_by_ensembl_id",
    "chain_expected": [
      "HPA_get_subcellular_location",
      "OpenTargets_get_target_gene_ontology_by_ensemblID"
    ],
    "steps": [
      {
        "query": "Human Protein Atlas basic gene info for EGFR by Ensembl ID",
        "expected_tools": [
          "HPA_get_gene_basic_info_by_ensembl_id"
        ],
        "tool_call": {
          "name": "HPA_get_gene_basic_info_by_ensembl_id",
          "arguments": {
            "ensembl_id": "ENSG00000146648"
          }
        },
        "validations": [
          {
            "type": "json_path_equals",
            "path": "Gene",
            "value": "EGFR"
          },
          {
            "type": "json_path_contains",
            "path": "Uniprot",
            "value": "P00533"
          }
        ]
      },
      {
        "query": "Subcellular location for EGFR protein",
        "expected_tools": [
          "HPA_get_subcellular_location"
        ],
        "tool_call": {
          "name": "HPA_get_subcellular_location",
          "arguments": {
            "gene_name": "EGFR"
          }
        },
        "validations": [
          {
            "type": "json_path_contains",
            "path": "main_locations",
            "value": "Plasma membrane"
          }
        ]
      },
      {
        "query": "EGFR protein interaction partners",
        "expected_tools": [
          "HPA_get_protein_interactions_by_gene",
          "OpenTargets_get_target_interactions_by_ensemblID"
        ],
        "tool_call": {
          "name": "OpenTargets_get_target_interactions_by_ensemblID",
          "arguments": {
            "ensemblId": "ENSG00000146648"
          }
        },
        "validations": [
          {
            "type": "json_path_non_empty",
            "path": "data.target.interactions.rows"
          }
        ]
      },
      {
        "query": "Gene ontology terms for EGFR target",
        "expected_tools": [
          "OpenTargets_get_target_gene_ontology_by_ensemblID"
        ],
        "tool_call": {
          "name": "OpenTargets_get_target_gene_ontology_by_ensemblID",
          "arguments": {
            "ensemblId": "ENSG00000146648"
          }
        },
        "validations": [
          {
            "type": "json_path_contains",
            "path": "data.target.geneOntology",
            "value": "plasma membrane"
          }
        ]
      }
    ]
  },
  {
    "id": "lung_carcinoma_targets",
    "title": "Lung carcinoma target discovery",
    "context": "Use Open Targets to prioritize lung carcinoma targets and verify evidence with literature.",
    "seed_tool": "OpenTargets_get_associated_targets_by_disease_efoId",
    "chain_expected": [
      "OpenTargets_get_target_interactions_by_ensemblID",
      "EuropePMC_search_articles"
    ],
    "steps": [
      {
        "query": "Targets associated with lung carcinoma (EFO_0001071)",
        "expected_tools": [
          "OpenTargets_get_associated_targets_by_disease_efoId"
        ],
        "tool_call": {
          "name": "OpenTargets_get_associated_targets_by_disease_efoId",
          "arguments": {
            "efoId": "EFO_0001071"
          }
        },
        "validations": [
          {
            "type": "json_path_list_any_equals",
            "path": "data.disease.associatedTargets.rows",
            "field": "target.approvedSymbol",
            "value": "EGFR"
          }
        ]
      },
      {
        "query": "Interaction partners for EGFR target",
        "expected_tools": [
          "OpenTargets_get_target_interactions_by_ensemblID"
        ],
        "tool_call": {
          "name": "OpenTargets_get_target_interactions_by_ensemblID",
          "arguments": {
            "ensemblId": "ENSG00000146648"
          }
        },
        "validations": [
          {
            "type": "json_path_non_empty",
            "path": "data.target.interactions.rows"
          }
        ]
      },
      {
        "query": "Literature evidence for EGFR lung carcinoma",
        "expected_tools": [
          "EuropePMC_search_articles"
        ],
        "tool_call": {
          "name": "EuropePMC_search_articles",
          "arguments": {
            "query": "EGFR lung carcinoma",
            "limit": 5
          }
        },
        "validations": [
          {
            "type": "json_path_non_empty",
            "path": "[0].title"
          },
          {
            "type": "json_path_contains",
            "path": "[0].title",
            "value": "EGFR"
          }
        ]
      }
    ]
  },
  {
